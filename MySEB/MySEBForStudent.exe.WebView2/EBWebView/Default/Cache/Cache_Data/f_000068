require(['jquery'], function($) {
    $('#single_select66f4e5b19626b3').change(function() {
        var ignore = $(this).find(':selected').attr('data-ignore');
        if (typeof ignore === typeof undefined) {
            $('#single_select_f66f4e5b19626b2').submit();
        }
    });
});
